<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="175dp"
        android:scaleType="fitXY"
        android:src="@drawable/normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,MissingConstraints" />

    <Button
        android:id="@+id/doctors"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn"
        android:backgroundTint="#FEFEFE"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/doctors"
        android:textAllCaps="false"
        android:textColor="#1433A4"
        android:textSize="18sp"
        app:iconTint="#FEFEFE"
        app:layout_constraintBottom_toTopOf="@+id/labAnalysis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="VisualLintButtonSize" />

    <Button
        android:id="@+id/labAnalysis"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn"
        android:backgroundTint="#FEFEFE"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/lab_analysis"
        android:textAllCaps="false"
        android:textColor="#1433A4"
        android:textSize="18sp"
        app:iconTint="#FEFEFE"
        app:layout_constraintBottom_toTopOf="@+id/radiations"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctors"
        tools:ignore="VisualLintButtonSize" />

    <Button
        android:id="@+id/radiations"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn"
        android:backgroundTint="#FEFEFE"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/radiations"
        android:textAllCaps="false"
        android:textColor="#1433A4"
        android:textSize="18sp"
        app:iconTint="#FEFEFE"
        app:layout_constraintBottom_toTopOf="@+id/surgeries"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labAnalysis"
        tools:ignore="VisualLintButtonSize" />

    <Button
        android:id="@+id/surgeries"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/surgeries"
        android:textAllCaps="false"
        android:textColor="#1433A4"
        android:textSize="18sp"
        app:iconTint="#FEFEFE"
        app:layout_constraintBottom_toTopOf="@+id/bill"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radiations"
        tools:ignore="VisualLintButtonSize" />

    <Button
        android:id="@+id/bill"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/bill"
        android:textAllCaps="false"
        android:textColor="#1433A4"
        android:textSize="18sp"
        app:iconTint="#FEFEFE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/surgeries"
        tools:ignore="VisualLintButtonSize" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/montserrat_semibold"
        android:text="@string/activity"
        android:textColor="#1433A4"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toEndOf="@+id/backButton"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <ImageView
        android:id="@+id/backButton"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginStart="50dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/arrow"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/check_your"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/doctors"
        app:layout_constraintEnd_toEndOf="@+id/doctors"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doctors"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />


</androidx.constraintlayout.widget.ConstraintLayout>