<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="24dp"
                android:background="@drawable/recommendation"
                android:fontFamily="@font/montserrat_semibold"
                android:paddingStart="137dp"
                android:paddingTop="12dp"
                android:paddingBottom="12dp"
                android:paddingEnd="8dp"
                android:text="@string/not_feeling_good_we_are_here_to_help_just_take_the_medical_assessment"
                android:textColor="#0F1498"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/imageView10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5"
                tools:ignore="TextSizeCheck" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="130dp"
                android:layout_height="0dp"
                android:layout_marginEnd="225dp"
                android:background="@drawable/reco"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="@+id/textView10"
                app:layout_constraintEnd_toEndOf="@+id/textView10"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView10"
                app:layout_constraintTop_toTopOf="@+id/textView10"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/navBar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:importantForAccessibility="no"
                android:scaleType="fitXY"
                android:src="@drawable/normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:id="@+id/imageButton23"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/frame2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="137dp"
                android:layout_height="159dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="26dp"
                android:src="@drawable/objects"
                app:layout_constraintBottom_toBottomOf="@+id/navBar"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/imageButton22"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:background="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/group43"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.706"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription,MissingConstraints" />

            <TextView
                android:id="@+id/textViewDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:fontFamily="@font/montserrat"
                android:text="@string/date"
                android:textColor="#1433A4"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageButton23" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:fontFamily="@font/montserrat_semibold"
                android:text="@string/hello"
                android:textColor="#1433A4"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewDate" />

            <TextView
                android:id="@+id/helloView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/montserrat_semibold"
                android:text="@string/text"
                android:textColor="#1433A4"
                android:textSize="24sp"
                app:layout_constraintStart_toEndOf="@+id/textView13"
                app:layout_constraintTop_toBottomOf="@+id/textViewDate" />


            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginBottom="40dp"
                android:fontFamily="@font/montserrat"
                android:text="@string/how_can_we_help_you_today"
                android:textColor="#1433A4"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/navBar"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/examinatons"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/examinations"
                app:layout_constraintEnd_toStartOf="@+id/analysis"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/clinicVisit"
                tools:ignore="ContentDescription,MissingConstraints" />

            <ImageView
                android:id="@+id/analysis"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:layout_marginTop="8dp"
                android:background="@null"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/labanalysis"
                app:layout_constraintEnd_toStartOf="@+id/firstaid"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/examinatons"
                app:layout_constraintTop_toBottomOf="@+id/doctorCall"
                tools:ignore="ContentDescription,MissingConstraints" />

            <ImageView
                android:id="@+id/firstaid"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:background="@null"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/firtaid"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/analysis"
                app:layout_constraintTop_toBottomOf="@+id/homeVisit"
                tools:ignore="ContentDescription,MissingConstraints" />

            <ImageView
                android:id="@+id/clinicVisit"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:layout_marginStart="8dp"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/clinicvisit"
                app:layout_constraintBottom_toTopOf="@+id/examinatons"
                app:layout_constraintEnd_toStartOf="@+id/doctorCall"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/navBar"
                tools:ignore="ContentDescription,MissingConstraints" />

            <ImageView
                android:id="@+id/doctorCall"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:background="@null"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/doctorcall"
                app:layout_constraintBottom_toTopOf="@+id/analysis"
                app:layout_constraintEnd_toStartOf="@+id/homeVisit"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/clinicVisit"
                app:layout_constraintTop_toBottomOf="@+id/navBar"
                tools:ignore="ContentDescription,MissingConstraints" />

            <ImageView
                android:id="@+id/homeVisit"
                android:layout_width="129dp"
                android:layout_height="170dp"
                android:layout_marginEnd="8dp"
                android:background="@null"
                android:paddingBottom="30dp"
                android:scaleType="fitCenter"
                android:src="@drawable/homevisit"
                app:layout_constraintBottom_toTopOf="@+id/firstaid"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/doctorCall"
                app:layout_constraintTop_toBottomOf="@+id/navBar"
                tools:ignore="ContentDescription,MissingConstraints" />

            <TextView
                android:id="@+id/clinicText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/clinic_visit"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/clinicVisit"
                app:layout_constraintEnd_toEndOf="@+id/clinicVisit"
                app:layout_constraintStart_toStartOf="@+id/clinicVisit"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/clinicText2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/radiations"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/examinatons"
                app:layout_constraintEnd_toEndOf="@+id/examinatons"
                app:layout_constraintStart_toStartOf="@+id/examinatons"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/doctorText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/doctor_call"
                android:textAlignment="center"
                android:textColor="#FBFBFB"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/doctorCall"
                app:layout_constraintEnd_toEndOf="@+id/doctorCall"
                app:layout_constraintStart_toEndOf="@+id/clinicText"
                app:layout_constraintStart_toStartOf="@+id/doctorCall"
                tools:ignore="MissingConstraints,TextContrastCheck,TextSizeCheck" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="375dp"
                android:layout_height="178dp"
                android:layout_marginBottom="60dp"
                android:adjustViewBounds="false"
                android:cropToPadding="false"
                android:scaleType="fitXY"
                android:src="@drawable/norm"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="ContentDescription,MissingConstraints,ImageContrastCheck" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="135dp"
                android:layout_height="135dp"
                android:layout_marginStart="8dp"
                android:src="@drawable/frame"
                app:layout_constraintBottom_toBottomOf="@+id/imageView10"
                app:layout_constraintStart_toStartOf="@+id/imageView10"
                app:layout_constraintTop_toTopOf="@+id/imageView10"
                app:layout_constraintVertical_bias="0.30"
                tools:ignore="ContentDescription,MissingConstraints" />

            <Button
                android:id="@+id/searchbtn"
                android:layout_width="205dp"
                android:layout_height="35dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/back"
                android:drawableStart="@drawable/ic_search"
                android:fontFamily="@font/montserrat"
                android:iconifiedByDefault="false"
                android:paddingStart="10dp"
                android:queryBackground="@null"
                android:queryHint="@string/search_for_medicine"
                android:scaleType="fitCenter"
                android:text="@string/search_for_medicine"
                android:textAllCaps="false"
                android:textColor="@color/white"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toTopOf="@+id/imageView10"
                tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,RtlSymmetry,TouchTargetSizeCheck" />


            <ImageView
                android:id="@+id/pxView"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="8dp"
                android:background="@null"
                android:scaleType="fitXY"
                android:src="@drawable/baseline_medication_242"
                app:layout_constraintEnd_toEndOf="@+id/ordermedicine"
                app:layout_constraintStart_toStartOf="@+id/ordermedicine"
                app:layout_constraintTop_toTopOf="@+id/ordermedicine"
                tools:ignore="ContentDescription,MissingConstraints,ImageContrastCheck" />

            <TextView
                android:id="@+id/ordermedicine"
                android:layout_width="85dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="13dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/back"
                android:fontFamily="@font/montserrat_light"
                android:paddingTop="50dp"
                android:paddingBottom="5dp"
                android:text="@string/order_nmedicine"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="11sp"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toBottomOf="@+id/searchbtn"
                tools:ignore="TextSizeCheck,TextContrastCheck" />


            <ImageView
                android:id="@+id/phoneView"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="12dp"
                android:background="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/px"
                app:layout_constraintEnd_toEndOf="@+id/sendPrescription"
                app:layout_constraintStart_toStartOf="@+id/sendPrescription"
                app:layout_constraintTop_toTopOf="@+id/sendPrescription"
                tools:ignore="ContentDescription,MissingConstraints,ImageContrastCheck" />

            <TextView
                android:id="@+id/sendPrescription"
                android:layout_width="85dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/back"
                android:fontFamily="@font/montserrat_light"
                android:paddingTop="50dp"
                android:paddingBottom="5dp"
                android:text="@string/send_prescription"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="11sp"
                app:layout_constraintStart_toEndOf="@+id/ordermedicine"
                app:layout_constraintTop_toBottomOf="@+id/searchbtn"
                tools:ignore="TextSizeCheck,TextContrastCheck" />

            <TextView
                android:id="@+id/doctorText3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/home_visit"
                android:textAlignment="center"
                android:textColor="#FBFBFB"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/homeVisit"
                app:layout_constraintEnd_toEndOf="@+id/homeVisit"
                app:layout_constraintStart_toStartOf="@+id/homeVisit"
                tools:ignore="MissingConstraints,TextContrastCheck,TextSizeCheck" />

            <TextView
                android:id="@+id/clinicText4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/analysis"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/analysis"
                app:layout_constraintEnd_toEndOf="@+id/analysis"
                app:layout_constraintStart_toEndOf="@+id/clinicText2"
                app:layout_constraintStart_toStartOf="@+id/analysis"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/clinicText5"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:text="@string/first_aid_instructions"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@+id/firstaid"
                app:layout_constraintEnd_toEndOf="@+id/firstaid"
                app:layout_constraintStart_toEndOf="@+id/clinicText4"
                app:layout_constraintStart_toStartOf="@+id/firstaid"
                tools:ignore="MissingConstraints,TextSizeCheck" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="24dp"
                android:background="@drawable/askbk"
                android:fontFamily="@font/montserrat_semibold"
                android:paddingStart="30dp"
                android:paddingTop="8dp"
                android:paddingEnd="30dp"
                android:paddingBottom="32dp"
                android:text="@string/ask"
                android:textColor="@color/main"
                android:textSize="19sp"
                app:layout_constraintBottom_toTopOf="@+id/textView10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/analysis"
                tools:ignore="TextSizeCheck" />

            <Button
                android:id="@+id/ask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:layout_marginBottom="4dp"
                android:background="@drawable/bluebtn"
                android:fontFamily="@font/montserrat_medium"
                android:text="@string/ask_now"
                android:textAllCaps="false"
                android:textColor="@color/white"
                app:iconTint="@color/main"
                app:layout_constraintBottom_toBottomOf="@+id/textView5"
                app:layout_constraintEnd_toEndOf="@+id/textView5"
                tools:layout_constraintRight_creator="1"
                tools:layout_constraintTop_creator="1" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</RelativeLayout>


